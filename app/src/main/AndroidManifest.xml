<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.musclebodybuilding">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".WorkoutActivity.ChestActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".WorkoutActivity.BicepActivity" />
        <activity android:name=".WorkoutActivity.TricepActivity" />
        <activity android:name=".WorkoutActivity.BackActivity" />
        <activity android:name=".WorkoutActivity.ShoulderActivity" />
        <activity android:name=".WorkoutActivity.AbsActivity" />
        <activity android:name=".WorkoutActivity.StretchingActivty" />
        <activity android:name=".WorkoutActivity.LegsActivity" />
        <activity android:name=".ChestWorkout.Butterfly">
        </activity>
        <activity android:name=".ChestWorkout.Cross_Cable"/>
        <activity android:name=".ChestWorkout.DeclineBenchBarbell"/>
    </application>

</manifest>